/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
        IN a, b;
        OUT out;

        PARTS:
        Nand(a=a, b=b, out=out0);
        Or(a=a, b=b, out=out1);
        And(a=out0, b=out1, out=out);
}
